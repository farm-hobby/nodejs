{% extends "templates/base.njk" %}

{% block content %}    
    <main>
        {% if users.length > 0 %}
            <h1>User List</h1>
            <ul>
                {% for user in users %}
                    <li>{{ user.name }}</li>
                {% endfor %}
            </ul>
        {% else %}
            <h1>No Users Founded!</h1>
        {% endif%}
    </main>
{% endblock %}
